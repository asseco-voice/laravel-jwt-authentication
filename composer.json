{
    "name": "asseco-voice/laravel-jwt-authentication",
    "description": "Package to decode and verify JWT tokens",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^10.0",
        "lcobucci/jwt": "^4.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.4.4",
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^8.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Auth\\AuthUserServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asseco\\Auth\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Asseco\\Auth\\": "src/"
        }
    }
}
